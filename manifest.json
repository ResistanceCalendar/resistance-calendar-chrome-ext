{
  "manifest_version": 2,

  "name": "Tockify event creator",
  "description": "Populate the Resistence Calendar's Tockify Form",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "notifications",
    "tabs",
    "http://calendar.resistancecalendar.org/*"
  ]
}
